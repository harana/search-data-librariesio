                  <div class="flex-auto pl-4 pt-2">
                    <dt class="text-sm font-semibold leading-6 text-gray-700">About</dt>
                  </div>
                  <div class="allow-select mt-2 flex w-full flex-none gap-x-4 border-t border-gray-900/5 px-6 py-6">
                  <%= project.openai_content.about ? JSON.parse(project.openai_content.about) : "" %>                    
                  </div>
                </dl>
              </div>
              <!-- Start Example Code -->
              <div class="shadow-sm ring-1 ring-gray-900/5 sm:mx-0 sm:rounded-lg mt-8">
                <dl class="flex flex-wrap">
                  <div class="flex-auto pl-4 pt-2">
                    <dt class="text-sm font-semibold leading-6 text-gray-700">Example Code</dt>
                  </div>
                  <div class="allow-select mt-2 w-full flex-none gap-x-4 border-t border-gray-900/5 px-6 py-6">
                  <%= project.openai_code %>
                  </div>
                </dl>
              </div>
              <!-- End Example Code -->
              <!-- Start Use Cases -->
              <div class="shadow-sm ring-1 ring-gray-900/5 sm:mx-0 sm:rounded-lg mt-8">
                <dl class="flex flex-wrap">
                  <div class="flex-auto pl-4 pt-2">
                    <dt class="text-sm font-semibold leading-6 text-gray-700">Uses Cases</dt>
                  </div>
                  <div class="allow-select mt-2 w-full flex-none gap-x-4 border-t border-gray-900/5 px-6 py-6">
                  <ul class="list-disc list-inside">
                  <% JSON.parse(project.openai_content.use_cases).each do |use_case| %>
                    <li><%= use_case %></li>
                  <% end %>
                  </ul>
                  </div>
                </dl>
              </div>
              <!-- End Use Cases -->
              <% end %>
            </section>
            <!-- End About -->

            <!-- Start Readme -->
            <section id="readme" class="tab-content hidden">
              <% if project.repository&.has_readme %>
                <%= project.repository.readme %>
              <% end %>
            </section>
            <!-- End Readme -->

            <!-- Start Versions -->
            <section id="versions" class="tab-content hidden">
              <ul role="list" class="divide-y divide-gray-100 overflow-hidden bg-white shadow-sm ring-1 ring-gray-900/5 sm:rounded-xl">
                <% project.versions.newest_first.each do |version| %>
                  <%= ERB.new(File.read('app/assets/harana/templates/_version_row.html.erb'), eoutvar: '_sub01').result(binding) %>
                <% end %>
                </ul>
            </section>
            <!-- End Versions -->

            <!-- Start Dependencies -->
            <section id="dependencies" class="tab-content hidden">
              <ul role="list" class="divide-y divide-gray-100 overflow-hidden bg-white shadow-sm ring-1 ring-gray-900/5 sm:rounded-xl">
              <% if project.latest_version %>
              <% project.latest_version.dependencies.each do |dependency| %>
                <%= ERB.new(File.read('app/assets/harana/templates/_dependency_row.html.erb'), eoutvar: '_sub01').result(binding) %>
              <% end %>
              </ul>
              <% end %>
            </section>
            <!-- End Dependencies -->

            <!-- Start Dependents -->
            <section id="dependents" class="tab-content hidden">
              <ul role="list" class="divide-y divide-gray-100 overflow-hidden bg-white shadow-sm ring-1 ring-gray-900/5 sm:rounded-xl">
                <% project.dependent_projects.uniq.each do |project| %>
                  <%= ERB.new(File.read('app/assets/harana/templates/_project_row.html.erb'), eoutvar: '_sub01').result(binding) %>
                <% end %>
                </ul>
            </section>
            <!-- End Dependents -->

            <!-- Start Contributors -->
            <section id="contributors" class="tab-content hidden">
              <ul role="list" class="grid grid-cols-1 gap-4 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4">
                <% project.contributors.each do |contributor| %>
                <li class="col-span-1 flex flex-col divide-y divide-gray-200 shadow-sm ring-1 ring-gray-900/5 sm:mx-0 sm:rounded-lg bg-white text-center">
                  <div class="flex flex-1 flex-col p-8">
                    <img class="mx-auto h-32 w-32 flex-shrink-0 rounded-full" src="<%= contributor.avatar_url %>" alt="">
                    <h3 class="mt-6 text-sm font-medium text-gray-900"><%= contributor.name %></h3>
                    <dl class="mt-1 flex flex-grow flex-col justify-between">
                      <dd class="text-sm text-gray-500"><%= contributor.company %></dd>
                    </dl>
                  </div>
                </li>
                <% end %>
              </ul>
            </section>
            <!-- End Contributors -->

            <!-- Start FAQs -->
            <section id="faqs" class="tab-content hidden">
              <% if project.openai_content %>
              <% JSON.parse(project.openai_content.faqs).each do |faq| %>
              <dl class="mb-10">
                <div>
                  <dt class="text-base font-semibold leading-7 text-gray-900"><%= faq['question'] %></dt>
                  <dd class="mt-2 text-base leading-7 text-gray-600"><%= faq['answer'] %></dd>
                </div>
              </dl>
              <% end %>
              <% end %>
            </section>
            <!-- End FAQs -->
          </div>
      </div>
    </main>
  </div>
</body>
</html>