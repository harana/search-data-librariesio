                <span><%= project.install_instructions %></span>
                <a onClick="navigator.clipboard.writeText('<%= project.install_instructions %>');">
                  <svg style="display: inline; margin: 0 0px 2px 8px;" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-4">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M15.666 3.888A2.25 2.25 0 0 0 13.5 2.25h-3c-1.03 0-1.9.693-2.166 1.638m7.332 0c.055.194.084.4.084.612v0a.75.75 0 0 1-.75.75H9a.75.75 0 0 1-.75-.75v0c0-.212.03-.418.084-.612m7.332 0c.646.049 1.288.11 1.927.184 1.1.128 1.907 1.077 1.907 2.185V19.5a2.25 2.25 0 0 1-2.25 2.25H6.75A2.25 2.25 0 0 1 4.5 19.5V6.257c0-1.108.806-2.057 1.907-2.185a48.208 48.208 0 0 1 1.927-.184" />
                  </svg>
                </a>
              </div>
              <div class="relative sm:hidden">
                <button type="button" class="-m-3 block p-3" id="more-menu-button" aria-expanded="false" aria-haspopup="true">
                  <span class="sr-only">More</span>
                  <svg class="h-5 w-5 text-gray-500" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                    <path d="M10 3a1.5 1.5 0 110 3 1.5 1.5 0 010-3zM10 8.5a1.5 1.5 0 110 3 1.5 1.5 0 010-3zM11.5 15.5a1.5 1.5 0 10-3 0 1.5 1.5 0 003 0z" />
                  </svg>
                </button>
              </div>
            </div>
          </div>
        </div>
        <div class="lg:pl-12">
          <div class="hidden sm:block">
            <div class="border-b border-gray-200">
              <nav id="#header-nav" class="-mb-px flex space-x-8" aria-label="Tabs">
                <% navbar_default = "cursor-pointer whitespace-nowrap border-b-2 py-4 px-1 text-sm font-medium" %>
                <a data-tab="about" onclick="switchTab(event, 'about')" class="<%= navbar_selected %> <%= navbar_default %>" aria-current="page">About</a>

                <% if project.repository&.has_readme %>
                <a data-tab="readme" onclick="switchTab(event, 'readme')" class="<%= navbar_unselected %> <%= navbar_default %>">Readme</a>
                <% end %>

                <% if project.versions.length > 0 %>
                <a data-tab="versions" onclick="switchTab(event, 'versions')" class="<%= navbar_unselected %> <%= navbar_default %>">Versions</a>
                <% end %>

                <% if project.latest_version and project.latest_version.dependencies.length > 0 %>
                <a data-tab="dependencies" onclick="switchTab(event, 'dependencies')" class="<%= navbar_unselected %> <%= navbar_default %>">Dependencies</a>
                <% end %>

                <% if project.dependents_count > 0 %>
                <a data-tab="dependents" onclick="switchTab(event, 'dependents')" class="<%= navbar_unselected %> <%= navbar_default %>">Dependents</a>
                <% end %>

                <% if project.contributors.length > 0 or (defined?(project.owners) and project.owners.length > 0) %>
                <a data-tab="contributors" onclick="switchTab(event, 'contributors')" class="<%= navbar_unselected %> <%= navbar_default %>">Contributors</a>
                <% end %>

                <a data-tab="faqs" onclick="switchTab(event, 'faqs')" class="<%= navbar_unselected %> <%= navbar_default %>">FAQs</a>
              </nav>
            </div>
          </div>
        </div>
      </header>

      <div class="overflow-scroll mx-auto px-12 py-12" style="height: calc(100vh - 120px);">
        <div class="mx-auto grid max-w-2xl grid-cols-1 grid-rows-1 items-start gap-x-8 gap-y-8 lg:mx-0 lg:max-w-none lg:grid-cols-4">
          <div class="lg:col-start-4 lg:row-end-1">

            <!-- Start Description -->
            <% if project.description.present? %>
            <div class="rounded-lg mb-4 bg-white shadow-sm ring-1 ring-gray-900/5">
              <dl class="flex flex-wrap">
                <div class="flex-auto pl-4 pt-2">
                  <dt class="text-sm font-semibold leading-6 text-gray-700">Description</dt>
                </div>
                <div class="mt-2 flex w-full flex-none gap-x-4 border-t border-gray-900/5 px-6 py-6">
                  <%= project.description %>
                </div>
              </dl>
            </div>
            <% end %>
            <!-- End Description -->

            <!-- Start Latest Version -->
            <% if project.latest_release_number.present? %>
            <h2 class="sr-only">Latest Version</h2>
            <div class="rounded-lg bg-gray-50 shadow-sm ring-1 ring-gray-900/5">
              <dl class="flex flex-wrap">
                <div class="flex-auto pl-6 py-6">
                  <dt class="text-sm font-semibold leading-6 text-gray-700">Latest Version</dt>
                  <dd class="allow-select mt-2 text-2xl leading-6 font-semibold text-gray-900"><%= project.latest_release_number %></dd>
                </div>
                <div class="flex-none self-end px-6 mb-6">
                  <dt class="sr-only">Status</dt>
                  <dd class="rounded-md bg-green-50 px-2 py-1 text-xs font-medium text-green-600 ring-1 ring-inset ring-green-600/20">Public</dd>
                </div>
              </dl>
            </div>
            <% end %>
            <!-- End Latest Version -->

            <!-- Start About -->
            <div class="rounded-lg mt-4 bg-white shadow-sm ring-1 ring-gray-900/5">
