                <% end %>
                <div class="mb-2 flex w-full flex-none"></div>
              </dl>
            </div>
            <% end %>
            <!-- End Links -->

            <!-- Start Tags -->
            <% unless project.keywords.empty? %>
            <div class="rounded-lg mt-4 bg-white shadow-sm ring-1 ring-gray-900/5">
              <dl class="flex flex-wrap">
                <div class="flex-auto pl-4 pt-2 pb-2">
                  <dt class="text-sm font-semibold leading-6 text-gray-700">Tags</dt>
                </div>
                <div class="border-t border-gray-900/5 px-6 py-6 tags-list">
                  <% project.keywords.each do |keyword | %>
                  <a href="tag.html"><span class="items-center rounded-md mr-1 bg-gray-50 px-2 py-1 text-xs font-medium text-gray-600 ring-1 ring-inset ring-gray-500/10"><%=  keyword  %></span></a>
                  <% end %>
                </div>
              </dl>
            </div>
            <%  end %>
            <!-- End Tags -->

            <!-- Start Github -->
            <div class="rounded-lg mt-4 bg-white shadow-sm ring-1 ring-gray-900/5">
              <dl class="flex flex-wrap">
                <div class="flex-auto pl-4 pt-2">
                  <dt class="text-sm font-semibold leading-6 text-gray-700">Github</dt>
                </div>
                <div class="mt-2 flex w-full flex-none gap-x-4 border-t border-gray-900/5 px-6 pt-6">
                  <dt class="flex-none">
                    <span class="sr-only">Watchers</span>
                    <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-5">
                      <path stroke-linecap="round" stroke-linejoin="round" d="M2.036 12.322a1.012 1.012 0 0 1 0-.639C3.423 7.51 7.36 4.5 12 4.5c4.638 0 8.573 3.007 9.963 7.178.07.207.07.431 0 .639C20.577 16.49 16.64 19.5 12 19.5c-4.638 0-8.573-3.007-9.963-7.178Z" />
